<h1>{{question}}</h1>

<div id="flot" style="width:500px;height:200px"></div>

<br/>
<h2>Voted:</h2>
{{#options}}
	<h3>{{text}}</h3>
	{{#voters}}
		{{name}}<br/>
	{{/voters}}
{{/options}}

<script type="text/javascript">
	$(document).ready(function() {
		$.plot($("#flot"), {{{flot_data}}}, {{{flot_options}}});
	});
</script>